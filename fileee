from math import *
x = [2,4,4,6]
y = [5,5,7,8]
distance = sum(abs(val1-val2) for val1, val2 in zip(x,y))
print("Manhattan distance from x to y: ",distance)

import math
x = [5, 6, 3, 1, 7, 2]
y = [4, 8, 9, 3, 2]
distance = math.sqrt(sum([(a - b) ** 2 for a, b in zip(x, y)]))
print("Euclidean distance from x to y: ",distance)
